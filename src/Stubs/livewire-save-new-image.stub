private function setNewImage(string $field, string $disk){
    if ($this->newImage)
    {
        Storage::disk($disk)->delete($this->editing->{$field});
        $this->editing->{$field} = $this->newImage->store('/', $disk);
    }
}

private function setSlug(string $field){
    $this->editing->slug = Str::slug($this->editing->{$field});
}
