
it('required :field: when edit :table:', function () {
    $model = :model:::factory()->create();
    :actingAs:
    Livewire::test(:livewire:::class)
        ->call('edit', $model->id)
        ->set('editing.:field:', '')
        ->call('save')
        ->assertHasErrors(['editing.:field:' => 'required']);
});
